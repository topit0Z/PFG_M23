#!/bin/bash
# DTE-Admin-uninstall
BIN_DIR=/usr/bin
LIB_DIR=/var/lib

#Carga de variables globales
source $LIB_DIR/DTE-Admin-globals

#Carga de funciones
source $BIN_DIR/DTE-Admin-funciones


#Limpieza del directorio de configuración
rm -rf $CONF_DIR/*

#Restauración de las copias de seguridad
restore_backups
